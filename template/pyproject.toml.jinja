    "lab-auto-pulumi{% endraw %}{{ lab_auto_pulumi_version }}{% raw %}",
    "boto3{% endraw %}{{ boto3_version }}{% raw %}",
    "boto3-stubs[all]{% endraw %}{{ boto3_version }}{% raw %}",
    "pydantic{% endraw %}{{ pydantic_version }}{% raw %}",
